<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true"
        default-init-method="init">
    <bean id="singer1" class="com.codrut.hooking_into_bean_creation.methods.SingerWithMethod">
        <property name="age" value="39" />
        <property name="name" value="John Mayers" />
    </bean>

    <bean id="singer2" class="com.codrut.hooking_into_bean_creation.methods.SingerWithMethod">
        <property name="age" value="29" />
    </bean>

    <bean id="singer3" class="com.codrut.hooking_into_bean_creation.methods.SingerWithMethod" />

</beans>